<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sjsucmpe131.erapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="org.sjsucmpe131.erappActivity.ERAppApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.sjsucmpe131.erappActivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.sjsucmpe131.erappActivity.UserDashboard"
            android:label="@string/title_activity_user_dashboard" >
        </activity>

        <activity
            android:name="org.sjsucmpe131.erappActivity.ViewExpense"
            android:label="@string/title_activity_view_expense" >
        </activity>
        <activity
            android:name="org.sjsucmpe131.erappActivity.UserRegister"
            android:label="@string/title_activity_user_register" >
        </activity>
          <activity
            android:name="org.sjsucmpe131.erappActivity.AddExpense"
            android:label="@string/button_user_dashboard_add_Expense" >
        </activity>
        
        <activity
            android:name="org.sjsucmpe131.erappActivity.ViewResultList"
            android:label="@string/title_activity_view_result_list"
            android:parentActivityName="android.app.ListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.app.ListActivity" />
        </activity>
        <activity
            android:name="org.sjsucmpe131.erappActivity.ReportResult"
            android:label="@string/title_activity_report_result" >
        </activity>
    </application>

</manifest>
